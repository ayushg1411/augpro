<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    
    <!----======== CSS ======== -->
    <link rel="stylesheet" href="/css/vendorform.css">
    <link rel="stylesheet" href="/css/button.css">
    <link rel="stylesheet" href="/css/card.css">
     
    <!----===== Iconscout CSS ===== -->
    <link rel="stylesheet" href="https://unicons.iconscout.com/release/v4.0.0/css/line.css">

    <title>Responsive Regisration Form </title> 

    <style>
        .container {
      display: flex;
      align-items: center;
      justify-content: center;
      flex-wrap: wrap;
    }
    
    .card_box {
      width: 400px;
      height: 200px;
      border-radius: 20px;
       background-color: lavender;
      position: relative;
      box-shadow: 0 25px 50px rgba(0,0,0,0.55);
      cursor: pointer;
      margin: 15px;
      transition: all .3s;
    }
    
    .card_box:hover {
      transform: scale(0.9);
    }
    
    .card_box span {
        padding: 12px;

      position: absolute;
      overflow: hidden;
      width: 380px;
      height: 100%;
    
    
    }
    
    
    </style>
</head>
<body>

    <%- include('partials/sidepanel2', {daata:dta}); %>


    <div style="display:flex; margin-top:100px; margin-bottom:20px; margin-left:30px; margin-right:30px; flex-direction:rows ">
     
       
        <a href="/parking"><button class="f-btn button-3" > Back</button></a>
      
    

      
     </div>
      

              <div style="  display:flex; justify-content: center; align-items: center;">
                <h1>parking details for <%=datas[0].vid%></h1>
              </div>



<div class="container">
     <%
     if(datas.length!=0){
     var i=1;
     datas.forEach(function(data){
     %>

        <div class="card_box">
        <span>
         <h3 style="background-color: rgb(210, 206, 214); margin-bottom: 5px; border-radius: 5px;">Parking Code &nbsp; &nbsp; &nbsp; &nbsp;<%=data.parkcode%></h3>
                <h3 style="background-color: rgb(210, 206, 214);  margin-bottom: 5px;  border-radius: 5px;"> Location Code &nbsp; &nbsp; &nbsp;<%= data.loc_code%></h3>
                <h3 style="background-color: rgb(210, 206, 214);  margin-bottom: 5px;  border-radius: 5px;"> Parking Name &nbsp; &nbsp; &nbsp;<%= data.pname%></h3>
                <h3 style="background-color: rgb(210, 206, 214);  margin-bottom: 5px;  border-radius: 5px;"> Area Name &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;<%= data.area%></h3>
             
       
        </span>
    </div>
    <%  i++; }) %>
    <% } else{ %>
      
      <h1>not found</h1>
  <% } %>
</div>


   

















     
  

</body>
